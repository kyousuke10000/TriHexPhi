    {
      "parameters": {
        "jsCode": "// Use intent classification code from common/code_classify_intent.md\nconst t = ($json.events[0].message.text || '').trim();\nlet intent = 'other';\nif (/[日月火水木金土]|(\d{1,2}[:時]\d{0,2})|場所|会場|開始|開場|参加/.test(t)) intent = 'event';\nelse if (/[?？]|いつ|どこ|どうやって|方法|値段|費用|誰/.test(t)) intent = 'question';\n$json.intent = intent;\nreturn $input.all();"
      },
      "id": "classify-intent",
      "name": "Classify Intent",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [680, 300]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "extract-title",
              "name": "title",
              "value": "={{ $json.events[0].message.text.split('\\n')[0] }}",
              "type": "string"
            },
            {
              "id": "extract-starts-at",
              "name": "starts_at",
              "value": "={{ $json.events[0].message.text.match(/\\d{4}-\\d{2}-\\d{2}\\s+\\d{2}:\\d{2}/)?.[0] }}",
              "type": "string"
            },
            {
              "id": "extract-location",
              "name": "location",
              "value": "={{ $json.events[0].message.text.match(/場所[:：]?\\s*(.+)/)?.[1] }}",
              "type": "string"
            },
            {
              "id": "extract-body",
              "name": "body",
              "value": "={{ $json.events[0].message.text }}",
              "type": "string"
            }
          ]
        }
      },
